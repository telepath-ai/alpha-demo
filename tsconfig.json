{
  "compilerOptions": {
    "sourceMap": true,
    "lib": ["es2019", "dom", "ES5"],
    "declaration": true,
    "declarationMap": true,
    "target": "es2019",
    "composite": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  }
}
